{
    "expo": {
        "name": "EatFresh",
        "slug": "eatfresh",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "light",
        "newArchEnabled": true,
        "splash": {
            "image": "./assets/splash-icon.png",
            "resizeMode": "contain",
            "backgroundColor": "#22c55e"
        },
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.eatfresh.app"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#22c55e"
            },
            "package": "com.eatfresh.app"
        },
        "web": {
            "favicon": "./assets/favicon.png",
            "bundler": "metro"
        },
        "scheme": "eatfresh",
        "plugins": [
            "expo-router",
            "expo-font"
        ]
    }
}